{
	"name": "sitecake/sitecake-cms",
    "description": "SiteCake - a simple, drag and drop CMS",
	"version": "2.0.3",
    "type": "project",
    "keywords": [ "cms" ],
    "homepage": "http://sitecake.com",
    "licence": "GPL-3.0+",

    "autoload": {
        "psr-4" : {
            "Sitecake\\": "application/sitecake"
        }
    },

    "require": {
        "silex/silex": "~1.2",
    	"pclzip/pclzip": "dev-master",
    	"electrolinux/phpquery": "dev-master",
    	"smottt/wideimage": "dev-master",
        "league/flysystem": "0.5.*"
    },

    "require-dev": {
        "phpunit/php-code-coverage": "1.2.*",
        "phpunit/php-invoker": "1.1.*",
        "phpunit/dbunit": "1.3.*",
        "phpunit/phpunit-selenium": "1.3.*",
        "phpunit/phpunit-story": "1.0.*",
    	"phpunit/phpunit": "3.7.*"
    }  
}