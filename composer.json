{
	"name": "sitecake/sitecake-cms",
    "description": "SiteCake - a simple, drag and drop CMS",
	"version": "2.0.3",
    "type": "project",
    "keywords": [ "cms" ],
    "homepage": "http://sitecake.com",
    "licence": "GPL-3.0+",

    "autoload": {
        "psr-4" : {
            "Sitecake\\": ["Sitecake", "src/Sitecake"]
        }
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nivancevic/flysystem"
        }
    ],

    "require": {
        "silex/silex": "~1.2",
        "symfony/translation": "~2.3",
        "symfony/config": "~2.3",
        "symfony/yaml": "~2.3",        
    	"pclzip/pclzip": "dev-master",
    	"electrolinux/phpquery": "dev-master",
    	"smottt/wideimage": "dev-master",
        "nivancevic/flysystem": "dev-master"
    },

    "require-dev": {
    	"phpunit/phpunit": "4.3.*"
    }  
}